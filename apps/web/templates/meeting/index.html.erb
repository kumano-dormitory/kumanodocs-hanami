<div class="p-strip">
  <div class="row">
    <h1>ブロック会議日程 一覧</h1>

    <ul class="p-list--divided">
      <% meetings.each do |meeting| %>
      <li class="p-list__item">
        <a href='<%= routes.meeting_path(id: meeting.id) %>' class="p-link--strong">
          <%= meeting.date %>のブロック会議
        </a>
      </li>
      <% end %>
    </ul>
  </div>
</div>
<div>
</div>
