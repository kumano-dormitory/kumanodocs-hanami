<div>
  <a href='<%= routes.new_article_path %>'>新規作成</a>
</div>
<div>
  <ul>
  <% articles_by_meeting.each do |meeting, articles| %>
    <li>
      <a href='<%= routes.meeting_path(id: meeting.id) %>'><%= meeting.date %></a>
      <ul>
      <% articles.each do |article| %>
        <li><a href='<%= routes.article_path(id: article.id) %>'><%= article_numbered_title(article) %></a></li>
      <% end %>
      </ul>
    </li>
  <% end %>
  </ul>
</div>
